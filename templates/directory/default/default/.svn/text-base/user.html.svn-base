<div id="showMBAttribute"></div>
{% if user %}
<h3>{{user.cn}}:</h3>
<table class="table table-striped table-bordered table-condensed">
	{% for key,data in user.items %}
		{% for ATTKey,ATT in attributes.items %}{% if ATTKey == key %}<tr><th>{{ATT}}</th><td><span class="{{ATTKey}}">{{data|safe}}</span></td><td>{% for ACLkey,ACLvalue in ACLs.items %} {% if ACLkey == key %}{% if ACLvalue.update %}{% for Virtualkey,virtual in virtuals.items %}{% if Virtualkey == key and not virtual %} <a href="#" onClick="showUserAttribute('{{user.RDN}}','{{key}}','{{objectID}}');"><i class="icon-pencil"></i></a> {% endif %}{% if ACLvalue.delete %}{% if Virtualkey == key %} <a href="#" onClick="deleteValue('{{user.RDN}}','{{objectID}}','{{key}}')"><i class="icon-trash"></i></a> {% endif %}{% endif %}{% endfor %}{% endif %}{% endif %}{% endfor %}</td></tr> {% endif %}{% endfor %}
	{% endfor %}
</table>

<!-- one-by-one value: -->
<table class="table table-striped table-bordered table-condensed">
<tr><th>{{attributes.uid}}</th><td>{{user.uid|safe}}</td>{% if ACLs.uid.update %}<td>M</td>{%endif %}{% if ACLs.uid.delete %} <td>X</td>{% endif %}</tr>
</table>
{% else %}
<div class="alert alert-error fade in"><button class="close" data-dismiss="alert">Ã—</button><center>{{message|safe}}</center></div>
{% endif %}
